# NativeYoutube Environment Configuration
# Copy this file to .env and fill in your values

# Apple Developer Account
APPLE_TEAM_ID=YOUR_TEAM_ID
APPLE_ID=your.email@example.com
APP_SPECIFIC_PASSWORD=your-app-specific-password

# Certificate
CERTIFICATE_PASSWORD=your-certificate-password
CERTIFICATE_NAME="Developer ID Application"

# GitHub
GITHUB_REPO=YOUR_USERNAME/YOUR_REPO

# Sparkle Update Framework
SPARKLE_FEED_URL=https://raw.githubusercontent.com/YOUR_USERNAME/YOUR_REPO/main/sparkle/appcast.xml